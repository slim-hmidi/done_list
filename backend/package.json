{
  "name": "done_list",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint ./src/**/*.ts --quiet",
    "lint-fix": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write ./src/**/*.ts",
    "migrate": "knex migrate:latest --knexfile ./build/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./build/knexfile.js",
    "seed": "knex seed:run --knexfile ./build/knexfile.js",
    "migrate:latest": "npm run rollback && npm run migrate && npm run seed",
    "start:build": "tsc -w",
    "start:run": "nodemon build/server.js",
    "start": "concurrently npm:start:*",
    "test": "set NODE_ENV=test && jest --runInBand --detectOpenHandles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix"
    }
  },
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@services": "build/services",
    "@loaders": "build/loaders",
    "@@types": "build/types",
    "@validations": "build/validations",
    "@config": "build/config",
    "@api": "build/api",
    "@constants": "build/constants",
    "@utils": "build/utils"
  },
  "dependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/supertest": "2.0.10",
    "@types/yup": "^0.29.11",
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "8.5.1",
    "knex": "^2.4.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "nodemon": "^2.0.7",
    "objection": "^2.2.15",
    "pg": "^8.5.1",
    "ts-node": "^9.1.1",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
